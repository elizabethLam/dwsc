<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="documentoProductorConsumidor" type="tipoDocumentoProductorConsumidor" />
	<xsd:complexType name="tipoDocumentoProductorConsumidor">
		<xsd:sequence>
			<xsd:element name="mensaje" type="tipoMensaje" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="tipoMensaje">
		<xsd:sequence>
			<xsd:element name="fecha" type="xsd:string" />
			<xsd:element name="descripcion_corta" type="xsd:string" />
			<xsd:element name="descripcion_larga" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>